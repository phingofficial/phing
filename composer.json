    "config": {
        "platform": {
            "php": "8.1"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phing/phing-composer-configurator": true,
            "ergebnis/composer-normalize": true,
            "symfony/flex": true
        }
    },
    "require": {
        "php": ">= 8.1",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "sebastian/version": "^3.0|^4.0|^5.0|^6.0",
        "symfony/console": "^5.3.10|^6.0|^7.0",
        "symfony/yaml": "^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-openssl": "*",
        "ext-pdo_sqlite": "*",
        "ext-phar": "*",
        "ext-sockets": "*",
        "ext-xsl": "*",
        "ext-zip": "*",
        "aws/aws-sdk-php": "^3.181",
        "ergebnis/composer-normalize": "^2.13",
        "friendsofphp/php-cs-fixer": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "jawira/plantuml-client": "^1.0",
        "jawira/plantuml-encoding": "^1.0",
        "mehr-als-nix/parallel": "^v1.0",
        "mikey179/vfsstream": "2.0.x-dev",
