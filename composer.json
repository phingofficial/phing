        "symfony/stopwatch": "^5.2|^6.0",
        "symplify/monorepo-builder": "^11.2",
        "tedivm/jshrink": "^1.3"
    },
    "replace": {
        "phing/task-analyzers": "self.version",
        "phing/task-apigen": "self.version",
        "phing/task-archives": "self.version",
        "phing/task-aws": "self.version",
