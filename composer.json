        "build",
        "tool",
        "task",
        "phing",
        "php",
        "build-tool",
        "build-automation",
        "dev",
        "ant",
        "make"
    ],
    "homepage": "https://www.phing.info/",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "Michiel Rook",
            "email": "mrook@php.net"
        },
        {
            "name": "Phing Community",
            "homepage": "https://github.com/phingofficial/phing/blob/main/CREDITS.md"
